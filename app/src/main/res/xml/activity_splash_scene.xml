<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@id/end_m"
        motion:constraintSetStart="@id/start"
        motion:duration="500"
        motion:motionInterpolator="bounce" />

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@id/end_e"
        motion:constraintSetStart="@id/end_m"
        motion:duration="500"
        motion:motionInterpolator="bounce" />

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@id/end_d"
        motion:constraintSetStart="@id/end_e"
        motion:duration="500"
        motion:motionInterpolator="bounce" />

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@id/end_k"
        motion:constraintSetStart="@id/end_d"
        motion:duration="500"
        motion:motionInterpolator="bounce" />

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@id/end_i"
        motion:constraintSetStart="@id/end_k"
        motion:duration="500"
        motion:motionInterpolator="bounce" />

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@id/end_t"
        motion:constraintSetStart="@id/end_i"
        motion:duration="500"
        motion:motionInterpolator="bounce" />

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@id/end_bag"
        motion:constraintSetStart="@id/end_t"
        motion:duration="500"
        motion:motionInterpolator="bounce" />

    <ConstraintSet android:id="@+id/start">
        <Constraint android:id="@id/m">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                motion:layout_constraintEnd_toEndOf="parent"
                motion:layout_constraintHorizontal_bias=".2"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintTop_toTopOf="parent" />
        </Constraint>

        <Constraint android:id="@id/e">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                motion:layout_constraintEnd_toEndOf="parent"
                motion:layout_constraintHorizontal_bias=".3"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintTop_toTopOf="parent" />
        </Constraint>

        <Constraint android:id="@id/d">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                motion:layout_constraintEnd_toEndOf="parent"
                motion:layout_constraintHorizontal_bias=".4"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintTop_toTopOf="parent" />
        </Constraint>

        <Constraint android:id="@id/k">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                motion:layout_constraintEnd_toEndOf="parent"
                motion:layout_constraintHorizontal_bias=".5"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintTop_toTopOf="parent" />
        </Constraint>

        <Constraint android:id="@id/i">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                motion:layout_constraintEnd_toEndOf="parent"
                motion:layout_constraintHorizontal_bias=".6"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintTop_toTopOf="parent" />
        </Constraint>

        <Constraint android:id="@id/t">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                motion:layout_constraintEnd_toEndOf="parent"
                motion:layout_constraintHorizontal_bias=".7"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintTop_toTopOf="parent" />
        </Constraint>

        <Constraint android:id="@id/bag">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                motion:layout_constraintEnd_toEndOf="parent"
                motion:layout_constraintHorizontal_bias=".82"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintTop_toTopOf="parent" />
        </Constraint>
    </ConstraintSet>

    <ConstraintSet
        android:id="@+id/end_m"
        motion:deriveConstraintsFrom="@id/start">
        <Constraint android:id="@id/m">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                motion:layout_constraintBottom_toBottomOf="parent"
                motion:layout_constraintEnd_toEndOf="parent"
                motion:layout_constraintHorizontal_bias=".2"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintTop_toTopOf="parent" />
        </Constraint>
    </ConstraintSet>

    <ConstraintSet
        android:id="@+id/end_e"
        motion:deriveConstraintsFrom="@id/end_m">
        <Constraint android:id="@id/e">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                motion:layout_constraintBottom_toBottomOf="parent"
                motion:layout_constraintEnd_toEndOf="parent"
                motion:layout_constraintHorizontal_bias=".3"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintTop_toTopOf="parent" />
        </Constraint>
    </ConstraintSet>

    <ConstraintSet
        android:id="@+id/end_d"
        motion:deriveConstraintsFrom="@id/end_e">
        <Constraint android:id="@id/d">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                motion:layout_constraintBottom_toBottomOf="parent"
                motion:layout_constraintEnd_toEndOf="parent"
                motion:layout_constraintHorizontal_bias=".4"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintTop_toTopOf="parent" />
        </Constraint>
    </ConstraintSet>

    <ConstraintSet
        android:id="@+id/end_k"
        motion:deriveConstraintsFrom="@id/end_d">
        <Constraint android:id="@id/k">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                motion:layout_constraintBottom_toBottomOf="parent"
                motion:layout_constraintEnd_toEndOf="parent"
                motion:layout_constraintHorizontal_bias=".5"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintTop_toTopOf="parent" />
        </Constraint>

    </ConstraintSet>

    <ConstraintSet
        android:id="@+id/end_i"
        motion:deriveConstraintsFrom="@id/end_k">
        <Constraint android:id="@id/i">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                motion:layout_constraintBottom_toBottomOf="parent"
                motion:layout_constraintEnd_toEndOf="parent"
                motion:layout_constraintHorizontal_bias=".6"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintTop_toTopOf="parent" />
        </Constraint>

    </ConstraintSet>

    <ConstraintSet
        android:id="@+id/end_t"
        motion:deriveConstraintsFrom="@id/end_i">
        <Constraint android:id="@id/t">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                motion:layout_constraintBottom_toBottomOf="parent"
                motion:layout_constraintEnd_toEndOf="parent"
                motion:layout_constraintHorizontal_bias=".7"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintTop_toTopOf="parent" />
        </Constraint>

    </ConstraintSet>

    <ConstraintSet
        android:id="@+id/end_bag"
        motion:deriveConstraintsFrom="@id/end_t">
        <Constraint android:id="@id/bag">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                motion:layout_constraintBottom_toBottomOf="parent"
                motion:layout_constraintEnd_toEndOf="parent"
                motion:layout_constraintHorizontal_bias=".82"
                motion:layout_constraintStart_toStartOf="parent"
                motion:layout_constraintTop_toTopOf="parent" />
        </Constraint>

    </ConstraintSet>

</MotionScene>